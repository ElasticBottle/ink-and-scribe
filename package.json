{
  "name": "trademark",
  "description": "A end to end automation for trademark filling in Singapore",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "env:set:prod": "func () { bunx dotenvx set ${1} ${2} -f ./.env.prod;}; func",
    "env:get:prod": "func () { bunx dotenvx get ${1} ${2} -f ./.env.prod;}; func",
    "env:set:local": "func () { bunx dotenvx set ${1} ${2} -f ./.env.local;}; func",
    "env:get:local": "func () { bunx dotenvx get ${1} ${2} -f ./.env.local;}; func",
    "with-env-local": "dotenvx run -f .env.local --",
    "with-env-prod": "dotenvx run --",
    "dev": "bun with-env-local next dev --port 6969",
    "build": "bun with-env-prod next build",
    "start": "bun with-env-prod next start",
    "clean": "git clean -xdf node_modules .turbo .next .sst",
    "format": "bunx @biomejs/biome format . --write",
    "lint": "bunx @biomejs/biome lint . --write"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.1.7",
    "framer-motion": "^11.3.21",
    "google-auth-library": "^9.15.1",
    "googleapis": "^144.0.0",
    "lucide-react": "^0.473.0",
    "next": "15.1.5",
    "next-themes": "^0.4.4",
    "posthog-js": "^1.207.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.2.1",
    "recharts": "^2.12.7",
    "rehype-pretty-code": "^0.14.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "sonner": "^1.7.2",
    "stripe": "^17.5.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "unified": "^11.0.5",
    "valibot": "^1.0.0-beta.12",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@dotenvx/dotenvx": "^1.33.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.7.2"
  },
  "packageManager": "bun@1.1.34"
}
